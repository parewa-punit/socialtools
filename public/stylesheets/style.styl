body
  margin: 0
  padding: 0
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif

a
  color: #00B7FF

#canvas
	position: relative

img
	max-width: 100%

.overlay
	position: absolute
	color: black

.ui-draggable-handle
	cursor: move

[contenteditable]:focus
	outline: 0px solid transparent

#wrapper .ui-widget-header
	height:5px
	background: orange

#exportContainer .ui-widget-header
	display: none
	
.watermark {
	color: #393939;
}